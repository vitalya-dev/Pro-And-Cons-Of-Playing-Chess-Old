{
	"workbench.colorTheme": "Clear Dawn Turbo",
	"workbench.iconTheme": "vs-minimal",
	"workbench.preferredDarkColorTheme": "Default Light+",
	"workbench.activityBar.visible": false,
	"editor.minimap.enabled": false,
	"editor.fontSize": 22,
	"editor.detectIndentation": false,
	"editor.folding": false,
	"editor.glyphMargin": false,
	"editor.smoothScrolling": true,
	"editor.quickSuggestions": {
		"other": false,
		"comments": false,
		"strings": false
	},
	"editor.suggestOnTriggerCharacters": false,
	"terminal.integrated.fontSize": 16,
	"editor.renderWhitespace": "none",
	"breadcrumbs.enabled": false,
	"files.autoSave": "off",
	"window.menuBarVisibility": "toggle",
	"editor.fontWeight": "bold",
	"editor.mouseWheelZoom": true,
	"workbench.editor.enablePreview": false,
	"editor.acceptSuggestionOnCommitCharacter": false,
	"workbench.statusBar.visible": false,
	"update.mode": "none",
	"extensions.autoCheckUpdates": false,
	"extensions.autoUpdate": false,
	"update.enableWindowsBackgroundUpdates": false,
	"editor.lineNumbers": "off",
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"editor.renderIndentGuides": false,
	"workbench.tree.renderIndentGuides": "none",
	"workbench.tips.enabled": false,
	"editor.codeLens": false,
	"csharp.referencesCodeLens.enabled": false,
	"editor.insertSpaces": false,
	"[csharp]": {
		"editor.defaultFormatter": "Leopotam.csharpfixformat",
	},
	"files.exclude": {
		"**/*.meta": true
	},
	"editor.rename.enablePreview": false,
	"editor.hover.enabled": true,
	"editor.hover.sticky": false,
	"editor.lightbulb.enabled": false,
	"editor.parameterHints.enabled": true,
	"editor.renderFinalNewline": false,
	"editor.foldingHighlight": false,
	"editor.highlightActiveIndentGuide": false,
	"editor.semanticHighlighting.enabled": false,
	"scm.diffDecorations": "none",
	"window.titleBarStyle": "native",
	"workbench.colorCustomizations": {
		"editorUnnecessaryCode.opacity": "#ffffff",
		"editorWidget.background": "#555555",
		"dropdown.background": "#ffffff",
		"dropdown.listBackground": "#555555",
		"dropdown.border": "#000000",
		"dropdown.foreground": "#000000",
		"list.highlightForeground": "#ffff55",
		"list.hoverBackground": "#aaaaaa",
		"pickerGroup.foreground": "#ffff55"

		// "input.background": "#ffffff",
		// "input.border": "#0000aa",
		// "input.foreground": "#ffffff",
		// "input.placeholderForeground": "#000000",
	},
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": [
					"storage.modifier",
					"entity.name.function",
					"constant.numeric",
					"constant.language",
					"string"
				],
				"settings": {
					"foreground": "#FFFFFF"
				}
			},
			{
				"scope": [
					"comment",
				],
				"settings": {
					"foreground": "#ffff55"
				}
			},
			{
				"scope": [
					"keyword",
					"keyword.control",
					"keyword.operator"
				],
				"settings": {
					"foreground": "#FFFFFF"
				}
			},
			
		]
	},
	"workbench.editor.showTabs": false,
	"workbench.editor.showIcons": false,
	"window.zoomLevel": 3,
	"editor.defaultFormatter": "Leopotam.csharpfixformat",
	"csharpfixformat.style.spaces.afterBracket": false,
	"csharpfixformat.style.spaces.afterParenthesis": false,
	"csharpfixformat.style.spaces.beforeParenthesis": false,
	"csharpfixformat.style.spaces.insideEmptyBraces": false,
	"csharpfixformat.style.spaces.removeAfterCommandBeforeParenthesis": "true"
}