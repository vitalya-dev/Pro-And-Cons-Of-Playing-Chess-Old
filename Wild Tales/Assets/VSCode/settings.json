{
	"workbench.colorTheme": "Delphi (Classic)",
	"workbench.iconTheme": "vs-minimal",
	"workbench.preferredDarkColorTheme": "Default Light+",
	"workbench.activityBar.visible": false,
	"editor.minimap.enabled": false,
	"editor.fontSize": 22,
	"editor.detectIndentation": false,
	"editor.folding": false,
	"editor.glyphMargin": false,
	"editor.smoothScrolling": true,
	"editor.quickSuggestions": {
		"other": false,
		"comments": false,
		"strings": false
	},
	"editor.suggestOnTriggerCharacters": false,
	"terminal.integrated.fontSize": 16,
	"editor.renderWhitespace": "none",
	"breadcrumbs.enabled": false,
	"files.autoSave": "off",
	"window.menuBarVisibility": "toggle",
	"editor.fontWeight": "bold",
	"editor.mouseWheelZoom": true,
	"workbench.editor.enablePreview": false,
	"editor.acceptSuggestionOnCommitCharacter": false,
	"workbench.statusBar.visible": false,
	"update.mode": "none",
	"extensions.autoCheckUpdates": false,
	"extensions.autoUpdate": false,
	"update.enableWindowsBackgroundUpdates": false,
	"editor.lineNumbers": "off",
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"editor.renderIndentGuides": false,
	"workbench.tree.renderIndentGuides": "none",
	"workbench.tips.enabled": false,
	"editor.codeLens": false,
	"csharp.referencesCodeLens.enabled": false,
	"editor.insertSpaces": false,
	"[csharp]": {
		"editor.defaultFormatter": "Leopotam.csharpfixformat",
	},
	"files.exclude": {
		"**/*.meta": true
	},
	"editor.rename.enablePreview": false,
	"editor.hover.enabled": true,
	"editor.hover.sticky": true,
	"editor.lightbulb.enabled": false,
	"editor.parameterHints.enabled": true,
	"editor.renderFinalNewline": false,
	"editor.foldingHighlight": false,
	"editor.highlightActiveIndentGuide": false,
	"editor.semanticHighlighting.enabled": false,
	"editor.colorDecorators": true,
	"scm.diffDecorations": "none",
	"workbench.colorCustomizations": {
		"editorHoverWidget.background": "#000080",
		"editorUnnecessaryCode.opacity": "#FFFFFF",
		"editor.lineHighlightBackground": "#000000",
	},
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": "entity.name.type",
				"settings": {
					"foreground": "#FFFFFF"
				}
			},
			{
				"scope": "entity.name.function",
				"settings": {
					"foreground": "#FFFFFF"
				}
			},
			{
				"scope": "punctuation",
				"settings": {
					"foreground": "#ffffff"
				}
			},
			{
				"scope": "entity.name.variable",
				"settings": {
					"foreground": "#ffffff"
				}
			},
			{
				"scope": "support.type.property-name",
				"settings": {
					"foreground": "#ffffff"
				}
			},
			{
				"scope": "string",
				"settings": {
					"foreground": "#ffffff"
				}
			},
			{
				"scope": "constant.numeric",
				"settings": {
					"foreground": "#ffffff"
				}
			}
		]
	},
	"workbench.editor.showTabs": false,
	"workbench.editor.showIcons": false
}